<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <diskStore path="d:/cache"/>

    <defaultCache
            maxElementsInMemory="1"
            eternal="false"
            overflowToDisk="true"
            timeToIdleSeconds="10"
            timeToLiveSeconds="20"
            diskPersistent="false"
            diskExpiryThreadIntervalSeconds="120"/>


</ehcache>

